        </activity>
        <activity android:name=".Book1" />
        <activity android:name=".Book2" />
        <activity android:name=".Book3" />
        <activity android:name=".Book4" />
        <activity android:name=".Book5" />
        <activity android:name=".Like" />
        <activity android:name=".Dislike"></activity>
    </application>

</manifest>
